{
  "extends": [
    "fbjs",
    "prettier"
  ],
  "plugins": [
    "prettier",
    "react"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn",
    "prettier/prettier": "error",
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
  }
}